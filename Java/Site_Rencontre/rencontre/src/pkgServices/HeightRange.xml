<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="pkgEntities.HeightRange">
	
	<select id="getListeHeightRanges" resultType="HeightRange">
		select *, start_range as "startRange",end_range as "endRange"
	    from height_range
	</select>
	

</mapper>